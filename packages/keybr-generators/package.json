{
  "private": true,
  "name": "@keybr/keybr-generators",
  "version": "0.0.0",
  "main": "lib/index.ts",
  "dependencies": {
    "@keybr/keyboard": "*",
    "@keybr/phonetic-model": "*",
    "@keybr/unicode": "*",
    "chalk": "^4.1.2"
  },
  "scripts": {
    "clean": "rm -fr .types",
    "compile": "tsc",
    "test": "ava",
    "generate-languages": "node -r @keybr/tsl ./lib/generate-languages.ts",
    "generate-layouts": "node -r @keybr/tsl ./lib/generate-layouts.ts"
  },
  "ava": {
    "extensions": [
      "ts",
      "tsx"
    ],
    "files": [
      "lib/**/*.test.*"
    ],
    "require": [
      "@keybr/tsl"
    ]
  }
}
